{
    // Fira Code, Consolas, Courier New, monospace
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "tabnine.experimentalAutoImports": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "workbench.colorCustomizations": {
        "sideBar.background": "#282a37",
        "editor.background": "#282a37",
        "terminal.background": "#282a37",
        "panel.background": "#282a37",
        "activeTab.background": "#282a37",
        "tab.activeBorderTop": "#ffb86c",
        "panelTitle.activeBorder": "#ffb86c",
        "editorCursor.foreground": "#a0f2fa",
        "terminalCursor.foreground": "#a0f2fa"
    },
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.cursorWidth": 2,
    "vscode_custom_css.imports": [
        "file:///C:/Extensions/Vscode/color.css"
    ],
    "vscode_custom_css.policy": true,
    "explorer.confirmDelete": false,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "Wscats.eno",
    },
    "[scss]": {
        "editor.defaultFormatter": "Wscats.eno"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    // Fira Code, Consolas, Courier New, monospace
    "editor.fontFamily": "SF Mono, Poppins, Source Code Pro, Input Mono, Inconsolata,  Arial, Helvetica, sans-serif",
    "editor.lineHeight": 30,
    "editor.fontLigatures": true,
    "editor.letterSpacing": 0.3,
    "editor.mouseWheelZoom": true,
    "workbench.colorTheme": "Dracula",
    "scss.lint.boxModel": "ignore",
    "scss.lint.fontFaceProperties": "ignore",
    "kite.showWelcomeNotificationOnStartup": true,
    "html.format.endWithNewline": true,
    "powermode.enabled": true,
    "packageManager": "npm",
    "powermode.enableStatusBarComboTimer": false,
    "vsicons.dontShowNewVersionMessage": true,
    "editor.fontSize": 16,
    "workbench.iconTheme": "vscode-icons",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell",
            "args": [
                "-nologo"
            ]
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "JavaScript Debug Terminal": {
            "extensionIdentifier": "ms-vscode.js-debug",
            "icon": "debug",
            "id": "extension.js-debug.debugTerminal",
            "title": "JavaScript Debug Terminal"
        }
    },
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            60
        ],
        "editor.defaultFormatter": "Dart-Code.dart-code",
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "dart.openDevTools": "flutter",
    "dart.showInspectorNotificationsForWidgetErrors": false,
    "gitlens.advanced.messages": {
        "suppressGitMissingWarning": true
    },
    "editor.formatOnPaste": true,
    "security.workspace.trust.untrustedFiles": "open",
    "bracketPairColorizer.depreciation-notice": false,
    "workbench.editor.enablePreview": false,
    "files.associations": {
        "*.scss": "scss",
        "*.js": "javascriptreact",
        "*.jsx": "javascriptreact",
        "*.blade.php": "html",
        ".tpl": "html",
        "*.module": "php"
    },
    "indentRainbow.colors": [
        "rgba(3,4,94,0.125)",
        "rgba(2,62,138,0.125)",
        "rgba(0,119,182,0.125)",
        "rgba(0,150,199,0.125)",
        "rgba(0,180,216,0.125)",
        "rgba(144,225,239,0.125)"
    ],
    "indentRainbow.tabmixColor": "rgba(128,32,96,0.6)",
    "color-highlight.markerType": "underline",
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "powermode.combo.timeout": 3600,
    "powermode.combo.location": "statusbar",
    "powermode.combo.counterEnabled": "hide",
    "powermode.combo.timerEnabled": "hide",
    "powermode.shake.enabled": false,
    "powermode.explosions.size": 3.5,
    "powermode.explosions.offset": 0.2,
    "powermode.explosions.duration": 1500,
    "terminal.integrated.fontFamily": "'MesloLGM NF'",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "typescript",
        "javascriptreact",
        "typescriptreact"
    ],
    "eslint.options": {
        "extensinons": [
            ".ts",
            ".js",
            ".tsx",
            ".jsx"
        ]
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "blade.format.enable": true,
    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
        "editor.formatOnSave": true
    },
    "js/ts.implicitProjectConfig.experimentalDecorators": true,
    "extensions.ignoreRecommendations": true,
    "emmet.syntaxProfiles": {},
    "beautify.options": {},
    "phpformatter.additionalExtensions": [],
    "phpformatter.arguments": [
        "--level=psr2"
    ],
    "editor.rulers": [],
    "eslint.codeActionsOnSave.mode": "problems",
    "eslint.codeAction.showDocumentation": {
        "enable": false
    },
    "eslint.format.enable": true,
    "eslint.codeAction.disableRuleComment": {
        "location": "separateLine"
    },
    "editor.formatOnSave": true,
    // "editor.cursorBlinking": "expand",
    // "editor.cursorSmoothCaretAnimation": true,
}